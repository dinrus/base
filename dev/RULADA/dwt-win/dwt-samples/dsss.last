# DWT dwt-samples top-level dsss.conf

[*]
buildflags+=-g -gc
buildflags+=-J$LIB_PREFIX/res -J..\..\lib\res

version(Windows) {
    # if no console window is wanted/needed use -version=gui
    version(gui) {
        buildflags+= -L/SUBSYSTEM:windows:5
    } else {
        buildflags+= -L/SUBSYSTEM:console:5
    }
    buildflags+= -L/rc:dwt
}

[client.d]
[server.d]
[test.d]
[examples]
type=subdir

[snippets]
type=subdir

[user]
type=subdir

version(DwtAddons){
    [jface]
    type=subdir
}



