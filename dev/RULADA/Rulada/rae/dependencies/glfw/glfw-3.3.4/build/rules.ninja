  command = cmd.exe /C "$PRE_LINK && E:\msys32\mingw32\bin\cc.exe $FLAGS  $LINK_FLAGS $in  -o $TARGET_FILE -Wl,--out-implib,$TARGET_IMPLIB -Wl,--major-image-version,0,--minor-image-version,0 $LINK_PATH $LINK_LIBRARIES && $POST_BUILD"
  description = Linking C executable $TARGET_FILE
  restat = $RESTAT


#############################################
# Rule for re-running cmake.

rule RERUN_CMAKE
  command = E:\msys32\usr\bin\cmake.exe -SD:\dinrus\dinrus32\dev\RELEASE\base-2021\base\dev\RULADA\Rulada\rae\dependencies\glfw\glfw-3.3.4 -BD:\dinrus\dinrus32\dev\RELEASE\base-2021\base\dev\RULADA\Rulada\rae\dependencies\glfw\glfw-3.3.4\build
  description = Re-running CMake...
  generator = 1


#############################################
# Rule for cleaning all built files.

rule CLEAN
  command = E:\msys32\mingw32\bin\ninja.exe -t clean
  description = Cleaning all built files...


#############################################
# Rule for printing all primary targets available.

rule HELP
  command = E:\msys32\mingw32\bin\ninja.exe -t targets
  description = All primary targets available:

